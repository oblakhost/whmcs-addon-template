{
  "name": "oblak/whmcs-addon",
  "version": "1.0.0",
  "type": "template",
  "license": "MIT",
  "require-dev": {
    "oblak/whmcs-dev-helper": "^1",
    "squizlabs/php_codesniffer": "^3.7",
    "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
  },
  "keywords": [
    "whmcs",
    "whmcs-addon"
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "module-install": [
      "module-install"
    ],
    "pack": [
      "@module-install",
      "pack"
    ],
    "post-create-project-cmd": [
      "@module-install"
    ]
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "oblak/whmcs-dev-helper": true
    }
  }
}
